{
  "compilerOptions": {
    "target": "es5",
    "declaration": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "WebWorker",
      "WebWorker.ImportScripts",
      "Webworker.Iterable"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "outDir": "dist",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "paths": {
      "atomic": ["./src/entry.ts"]
    }
  },
  "include": [
    "src"
  ]
}
