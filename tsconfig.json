{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "node",
        "target": "es6",
        "lib": [
            "DOM",
            "ES6",
            "DOM.Iterable",
            "ScriptHost",
            "ESNext"
        ],
        "jsx": "react",
        "esModuleInterop": true,
        "strict": true,
        "sourceMap": true,
        "noEmitOnError": true,
        "baseUrl": ".",
        "resolveJsonModule": true,
        "outDir": "out"
    }
}